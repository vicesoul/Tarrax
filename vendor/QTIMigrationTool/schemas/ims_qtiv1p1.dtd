                    %I_Embedded;
                    %I_Width;
                    %I_Y0;
                    %I_X0;
                    %I_EntityRef; >
<!---->
<!---->
<!--	++++++++++++++++++++++++++++++++++++++++++++++++++++++	-->
<!--	Explanation:					-->
