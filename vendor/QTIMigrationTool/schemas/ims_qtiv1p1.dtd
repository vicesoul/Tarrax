<!--		specification			.	-->
<!--	Type:					-->
<!--		PCDATA				-->
<!--	Notes:					-->
<!--						-->
<!--						-->
<!--	++++++++++++++++++++++++++++++++++++++++++++++++++++++	-->
<!ELEMENT matemtext  (#PCDATA )>
<!ATTLIST matemtext  texttype    CDATA  'text/plain'
                     %I_Label;
                     %I_CharSet;
                     %I_Uri;
                     xml:space    (Preserved | Default )  'Default'
                     xml:lang    CDATA  #IMPLIED
                     %I_EntityRef; >
